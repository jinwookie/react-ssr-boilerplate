{
  "name": "react-ssr-boilerplate",
  "version": "1.0.1",
  "main": "index.js",
  "author": "jinwookie <jinwookie@hotmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jinwookie/react-ssr-boilerplate.git"
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "compression": "^1.7.3",
    "concurrently": "^4.1.0",
    "express": "^4.16.4",
    "immutable": "^4.0.0-rc.12",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^4.6.3",
    "react-redux": "^6.0.0",
    "react-router-config": "^4.4.0-beta.6",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-immutable": "^4.0.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-dynamic-import-node": "^2.0.0",
    "css-loader": "^2.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.9",
    "null-loader": "^0.1.1",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-manifest-plugin": "^2.0.4",
    "webpack-node-externals": "^1.7.2"
  },
  "scripts": {
    "clean": "rm -rf dist & rm -rf build",
    "start": "npm run server:prod",
    "server:prod": "NODE_ENV=production node ./build/server.js",
    "server:build": "NODE_ENV=production BABEL_ENV=server webpack --config webpack.server.config.js",
    "server:dev:build": "NODE_ENV=development BABEL_ENV=server webpack --config webpack.server.config.js",
    "server:dev:run": "NODE_ENV=development nodemon ./build/server.js",
    "server:dev": "concurrently --kill-others \"npm run server:dev:build\" \"npm run server:dev:run\"",
    "client:dev": "NODE_ENV=development webpack-dev-server",
    "client:build": "NODE_ENV=production webpack",
    "build": "npm run client:build && npm run server:build",
    "dev": "concurrently --kill-others \"npm run server:dev\" \"npm run client:dev\""
  }
}
