{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Brian Lee",
  "license": "MIT",
  "dependencies": {
    "@babel/polyfill": "^7.0.0-beta.47",
    "compression": "^1.7.2",
    "concurrently": "^3.5.1",
    "express": "^4.16.3",
    "immutable": "^3.8.2",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^4.2.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.0",
    "redux-immutable": "^4.0.0",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.47",
    "@babel/preset-env": "^7.0.0-beta.47",
    "@babel/preset-react": "^7.0.0-beta.47",
    "@babel/preset-stage-3": "^7.0.0-beta.47",
    "babel-loader": "^8.0.0-beta.3",
    "babel-plugin-dynamic-import-node": "^2.0.0",
    "css-loader": "^0.28.11",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.9.0",
    "nodemon": "^1.17.4",
    "null-loader": "^0.1.1",
    "optimize-css-assets-webpack-plugin": "^4.0.1",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^3.1.4",
    "webpack-node-externals": "^1.7.2",
    "webpack-serve": "^1.0.2"
  },
  "scripts": {
    "clean": "rm -rf dist & rm -rf build",
    "start": "yarn run server:prod",
    "server:prod": "NODE_ENV=production node ./build/server.js",
    "server:build": "NODE_ENV=production BABEL_ENV=server webpack --config webpack.server.config.js",
    "server:dev:build": "NODE_ENV=development BABEL_ENV=server webpack --config webpack.server.config.js",
    "server:dev:run": "NODE_ENV=development nodemon ./build/server.js",
    "server:dev": "concurrently --kill-others \"yarn run server:dev:build\" \"yarn run server:dev:run\"",
    "client:dev": "NODE_ENV=development webpack-dev-server",
    "client:build": "NODE_ENV=production webpack",
    "build": "yarn run server:build & yarn run client:build",
    "dev": "concurrently --kill-others \"yarn run server:dev\" \"yarn run client:dev\""
  }
}
